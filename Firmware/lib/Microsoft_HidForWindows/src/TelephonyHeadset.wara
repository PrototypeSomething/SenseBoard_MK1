[[settings]]
generateCpp = true
cppDescriptorName = "TelephonyHeadsetDescriptor"

[[applicationCollection]]
usage = ['Telephony Device', 'Headset']

    [[applicationCollection.inputReport]]
    name = "TelephonyHeadsetHookSwitchReport"

        [[applicationCollection.inputReport.variableItem]]
        usage = ['Telephony Device', 'Hook Switch']
        logicalValueRange = [0, 1]

    [[applicationCollection.inputReport]]
    name = "TelephonyHeadsetPhoneMuteReport"

        [[applicationCollection.inputReport.variableItem]]
        usage = ['Telephony Device', 'Phone Mute']
        logicalValueRange = [0, 1]

    [[applicationCollection.inputReport]]
    name = "TelephonyHeadsetRedialReport"

        [[applicationCollection.inputReport.variableItem]]
        usage = ['Telephony Device', 'Redial']
        logicalValueRange = [0, 1]

    [[applicationCollection.inputReport]]
    name = "TelephonyHeadsetFlashReport"

        [[applicationCollection.inputReport.variableItem]]
        usage = ['Telephony Device', 'Flash']
        logicalValueRange = [0, 1]

    [[applicationCollection.inputReport]]
    name = "TelephonyHeadsetSendReport"

        [[applicationCollection.inputReport.variableItem]]
        usage = ['Telephony Device', 'Send']
        logicalValueRange = [0, 1]

    [[applicationCollection.inputReport]]
    name = "TelephonyHeadsetDropReport"

        [[applicationCollection.inputReport.variableItem]]
        usage = ['Telephony Device', 'Drop']
